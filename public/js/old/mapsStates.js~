// Make sure to include the `ui.router` module as a dependency.
angular.module('maps')
  .config(
    [ '$stateProvider', '$urlRouterProvider',
      function ($stateProvider, $urlRouterProvider) {   
        $urlRouterProvider
          .otherwise('/');

        $stateProvider
      	  .state('home', {
      	      url:'/',
      	      template:''
      	  })
      }
    ]
  );


